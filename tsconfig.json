{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@content/*": ["content/*"],
      "@utils/*": ["src/utils/*"],
      "@community/*": ["content/community/*"],
      "@docs/*": ["content/docs/*"]
    }
  },
  "include": ["./src/**/*"]
}
